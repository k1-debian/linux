#include <dt-bindings/pinctrl/ingenic-pinctrl.h>

&pinctrl {
	uart0_pin: uart0-pin {
		uart0_pc: uart0-pc {
			ingenic,pinmux = <&gpc 21 24>;
			ingenic,pinmux-funcsel = <PINCTL_FUNCTION1>;
		};
